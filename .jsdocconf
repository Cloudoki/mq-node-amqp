{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "src/"
    ],
    "exclude": [
      "node_modules/",
      "docs/",
      "coverage/",
      "test/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
